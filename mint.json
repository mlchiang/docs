{
  "$schema": "https://mintlify.com/schema.json",
  "name": "LeagueSync.dev",
  "logo": {
    "dark": "/logo/dark.svg",
    "light": "/logo/light.svg"
  },
  "favicon": "/favicon.svg",
  "colors": {
    "primary": "#218DBE",
    "light": "#218DBE",
    "dark": "#218DBE",
    "anchors": {
      "from": "#218DBE",
      "to": "#218DBE"
    }
  },
  "topbarLinks": [
    {
      "name": "Support",
      "url": "mailto:michael@fantasyplaybook.ai"
    }
  ],
  "topbarCtaButton": {
    "name": "FantasyPlaybook",
    "url": "https://fantasyplaybook.ai"
  },
  "tabs": [
    {
      "name": "API Reference",
      "url": "api-reference"
    }
  ],
  "anchors": [
    {
      "name": "Changelog",
      "icon": "newspaper",
      "url": "https://app.fantasyplaybook.ai/changelog"
    }
  ],
  "navigation": [
    {
      "group": "Get Started",
      "pages": [
        "introduction",
        "quickstart",
        "development"
      ]
    },
    {
      "group": "API Documentation",
      "pages": [
        "api-reference/introduction"
      ]
    },
    {
      "group": "Status",
      "pages": [
        "api-reference/status/health_check"
      ]
    },
    {
      "group": "League",
      "pages": [
        "api-reference/league/findAllLeagues",
        "api-reference/league/{league_id}",
        "api-reference/league/settings/{league_id}",
        "api-reference/league/standings/{league_id}"
      ]
    },
    {
      "group": "Roster",
      "pages": [
        "api-reference/roster/getAllRosters/{league_id}",
        "api-reference/roster/getRoster/{league_id}/{roster_id}"
      ]
    },
    {
      "group": "Players",
      "pages": [
        "api-reference/players/getAllRostered/{league_id}/player_id",
        "api-reference/players/getAllRostered/{league_id}/names",
        "api-reference/players/{league_id}/searchPlayer/name/{player_name}",
        "api-reference/players/{league_id}/searchPlayer/player_id/{player_id}"
      ]
    },
    {
      "group": "Transactions",
      "pages": [
        "api-reference/transactions/{league_id}"
      ]
    },
    {
      "group": "Matchups",
      "pages": [
        "api-reference/matchups/{league_id}/{date_interval}"
      ]
    }
  ],
  "footerSocials": {
    "twitter": "https://twitter.com/mintlify",
    "github": "https://github.com/mintlify",
    "linkedin": "https://www.linkedin.com/company/mintsearch"
  }
}